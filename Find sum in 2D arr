  function matrixSum(arr) {
  // You only need to implement this function.
  // let sum =0;
  // for(let i=0; i<arr.length; i++){
  //   for(let j=0; j<arr[i].length; j++)
  //    sum += arr[i][j];
  // }
  // return sum;
  //tc-O(n*m) sc-O(1)

  return arr.flat().reduce((sum,curr)=>sum+curr,0);
  //tc-O(n*m) sc-O(n*m);
}
